import '../../../src/parser/TextParser.dart';
import '../../../src/validition/validition.dart';
import 'package:test/test.dart';
import '../../../src/Util/GlobalConstants';

class TestValidate {
  static const String className = 'TestValidate';

  void testValidate(String input) {
    GlobalConstants.TESTING = true;
    test('Validate Input', () {
      String str = input;
      Validation validation = Validation();
      var output = validation.validate(str);

      expect(output, equals(true));
      expect(output, true);
      expect(output is bool, true);
      expect(output is String, false);
    });
  }
}